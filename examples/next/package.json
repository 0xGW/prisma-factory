{
  "name": "@prisma-factory/next-example",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:echobind/prisma-factory.git",
    "directory": "examples/next"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "prisma": {
    "seed": "ts-node scripts/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:update": "yarn test --updateSnapshot",
    "test:watch": "yarn test --watch",
    "clean": "rimraf .turbo .next node_modules prisma/client",
    "db:start": "docker-compose up -d",
    "db:generate": "prisma generate"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.5",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@prisma/client": "^3.10.0",
    "framer-motion": "^6.2.8",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@prisma-factory/config": "workspace:^",
    "@prisma-factory/test-utils": "workspace:^",
    "@prisma-factory/tsconfig": "workspace:^",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "eslint": "8.9.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "jest": "^27.5.1",
    "msw": "^0.38.1",
    "next-router-mock": "^0.6.5",
    "next-transpile-modules": "9.0.0",
    "prisma": "^3.10.0",
    "prisma-factory": "workspace:^",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
