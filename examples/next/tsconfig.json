{
  "extends": "@prisma-factory/tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "test-utils": ["test/utils"]
    }
  },
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    },
    "pretty": true,
    "transpileOnly": true,
    "require": ["tsconfig-paths/register"]
  },
  "exclude": ["node_modules", "prisma/client"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
