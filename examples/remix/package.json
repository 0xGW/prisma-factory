{
  "name": "remix-example",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:echobind/prisma-factory.git",
    "directory": "examples/remix"
  },
  "prisma": {
    "seed": "ts-node scripts/seed.ts"
  },
  "scripts": {
    "build": "remix build",
    "dev": "PORT=3002 remix dev",
    "postinstall": "rm -f node_modules/remix/index.d.ts && remix setup node",
    "start": "remix-serve build",
    "clean": "rimraf .turbo .cache build public/build node_modules prisma/client prisma/generated",
    "db:start": "docker-compose up -d",
    "db:generate": "prisma generate"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.5",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@prisma/client": "^3.10.0",
    "@remix-run/react": "^1.2.2",
    "@remix-run/serve": "^1.2.2",
    "framer-motion": "^6.2.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "remix": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@prisma-factory/test-utils": "workspace:^",
    "@remix-run/dev": "^1.2.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "prisma": "^3.10.0",
    "prisma-factory": "workspace:^",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  },
