{
  "name": "prisma-factory",
  "version": "0.1.0",
  "description": "Generates factories for your prisma models",
  "bin": "./dist/src/generator.js",
  "scripts": {
    "build": "tsc -p .",
    "dev:watch": "tsc -p . --watch",
    "dev": "tsc -p . && yarn prisma:generate",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:status": "npx prisma migrate status",
    "migrate:resolve": "npx prisma migrate resolve",
    "prisma:generate": "npx prisma generate",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": "git@github.com:echobind/prisma-factory.git",
  "contributors": [
    "Chris Ball <chris@echobind.com>"
  ],
  "license": "MIT",
  "private": false,
  "keywords": [
    "prisma",
    "generator",
    "factory",
    "factories"
  ],
  "dependencies": {
    "@prisma/generator-helper": "^2.26.0",
    "@prisma/sdk": "^2.26.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@prisma/client": "^2.26.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/node": "^16.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "prisma": "^2.26.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "ts-morph": "^11.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
