{
  "pipeline": {
    "db:generate": {
      "dependsOn": ["^build"],
      "outputs": ["node_modules/.prisma/client", "node_modules/.prisma-factory/factories"]
    },
    "build": {
      "dependsOn": ["db:generate", "^build"],
      "outputs": ["bin/**", "dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "cache": false,
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
