{
  "name": "prisma-factory",
  "version": "0.0.0",
  "description": "Generates factories for your prisma models",
  "repository": "git@github.com:echobind/prisma-factory.git",
  "contributors": [],
  "license": "MIT",
  "keywords": [
    "prisma",
    "generator",
    "factory",
    "factories"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./dist/generator.js",
  "scripts": {
    "dev": "tsup src/index.ts src/generator.ts --format cjs,esm --watch --onSuccess \"yarn prisma:generate\"",
    "build": "tsup src/index.ts src/generator.ts --format cjs,esm --dts-resolve",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "clean": "rimraf .turbo node_modules dist yarn-error.log",
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:status": "prisma migrate status",
    "migrate:resolve": "prisma migrate resolve",
    "db:generate": "prisma generate",
    "db:start": "docker-compose up -d",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "peerDependencies": {
    "@prisma/client": "*",
    "typescript": ">4.5.2"
  },
  "dependencies": {
    "@prisma/generator-helper": "^3.9.1",
    "@prisma/sdk": "^3.9.1",
    "change-case": "^4.1.2",
    "ts-morph": "^13.0.3"
  },
  "devDependencies": {
    "@prisma/client": "^3.9.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "config": "workspace:*",
    "eslint": "^8.8.0",
    "execa": "^6.0.0",
    "jest": "^27.4.7",
    "prisma": "^3.9.1",
    "release-it": "^14.12.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tsconfig": "workspace:*",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5",
    "vitest": "^0.2.7"
  }
}
